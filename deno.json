{
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "dev": "deno run -A --watch main.ts",
    "start": "deno run --allow-net --allow-read main.ts"
  },
  "fmt": {
    "semiColons": false
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
